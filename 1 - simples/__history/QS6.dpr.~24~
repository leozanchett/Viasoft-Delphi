program QS6;

uses
  uISubstitui in 'uISubstitui.pas',
  uSubstitui in 'uSubstitui.pas';

{$R *.RES}
var
   LSubstitui: ISubstitui;
   LResultado, a: String;
const 
   XUXA = 'xuxa a sasha sumiu, me ajude achar a sasha';
begin
   LSubstitui := TSubstitui.New;
   LResultado := LSubstitui.Substituir(XUXA, 'sa', 'nata');
end.

